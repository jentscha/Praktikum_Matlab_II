function [ A, B, C, D, M_AP ] = linearisierung( f, h, AP )
%LINEARISIERUNG Summary of this function goes here
%   Detailed explanation goes here
syms phi1 phi2 dphi1 dphi2 ddphi1 ddphi2 M;

x = [phi1;dphi1;phi2;dphi2];
u = M;

A = jacobian(f,x);
B = jacobian(f,u);
C = jacobian(h,x);
D = jacobian(h,u);

A = subs(A,x,AP);
B = subs(B,x,AP);
C = subs(C,x,AP);
D = subs(D,x,AP);




% A = double(A);
% B = double(B);
% C = double(C);
% D = double(D);

end

